{
	"name": "webview-rpc",
	"workspaces": [
		"packages/*"
	],
	"private": true,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/QiyuanChen02/webview-rpc.git"
	},
	"scripts": {
		"build": "npm run build -ws --if-present",
		"distribute": "npm publish -ws",
		"dev": "npm run dev -ws --if-present",
		"format": "npx @biomejs/biome format --write packages"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18"
	},
	"author": "Qiyuan Chen",
	"license": "ISC",
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/node": "^24.6.2",
		"@types/vscode": "^1.104.0",
		"tsdown": "^0.15.9",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"tslib": "^2.8.1",
		"zod": "^4.1.11"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
